<div id="form">
  <h2>Signup</h2>
  <form action="/auth/signup" method="POST">
    <label> Email
      <input type="email" name="username" placeholder="rockstar@ironhack.com" required  />
    </label>
    <label>Password
      <input type="password" name="password" placeholder="********" required />
    </label>
    <button type="submit">Signup</button>
    {{#if errorMessage}}
      <p class="error">{{errorMessage}}</p>
    {{/if}}
  </form>
</div>